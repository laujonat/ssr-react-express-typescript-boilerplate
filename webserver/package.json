{
  "name": "webserver",
  "version": "1.0.0",
  "description": "flask express ssr",
  "main": "./server/index.ts",
  "dependencies": {
    "@types/react": "^16.8.20",
    "express": "^4.17.1",
    "html-minifier": "^4.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "tslib": "^1.10.0",
    "uglify-js": "^3.6.0"
  },
  "peerDependencies": {
    "@babel/core": "@^7.0.0",
    "@types/react": "^16.8.20"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/express": "^4.17.0",
    "@types/node": "^12.0.8",
    "@types/prop-types": "^15.7.1",
    "@types/react-dom": "^16.8.4",
    "@types/terser-webpack-plugin": "^1.2.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "fork-ts-checker-webpack-plugin": "^1.3.7",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "preload-webpack-plugin": "^2.3.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "terser-webpack-plugin": "^1.3.0",
    "ts-loader": "^6.0.2",
    "ts-node": "^8.3.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.2",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "debug": "npm run clean && webpack --progress --colors --profile --display-modules --display-reasons --display-error-details",
    "build": "npm-run-all -s clean lint build:tsc",
    "build:stats": "npm run build && webpack -p --json > stats.json",
    "clean": "rimraf ./build && mkdir ./build",
    "lint": "tslint -c tslint.json \"./client/**/*.(ts|tsx)\" \"./server/**/*.(ts|tsx)\"",
    "lint:fix": "tslint --fix -c tslint.json \"./client/**/*.ts\" \"./server/**/*.ts\"",
    "build:prod": "npm run build && npm install --only=prod ",
    "start": "npm run build:prod && NODE_ENV=production && webpack -p --config=webpack.config.js ./server/index.ts && npm run watch:ts",
    "start:dev": "npm run build && webpack -d && npm run watch:ts",
    "start:debug": "run-s debug",
    "build:tsc": "tsc",
    "watch:ts": "nodemon --config \"./nodemon.json\""
  },
  "keywords": [
    "flask",
    "ssr",
    "react"
  ],
  "author": "",
  "license": "MIT"
}
